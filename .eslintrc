{
  "extends": [
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react"
  ],
  "env": {
    "es6": true,
    "node": true
  },
  "globals": {
    "__DEV__": true,
  },
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  }
}
